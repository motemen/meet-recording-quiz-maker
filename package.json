{
  "name": "meet-recording-quiz-maker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b",
  "scripts": {
    "dev": "NODE_ENV=development tsx src/index.tsx",
    "build:client": "vite build",
    "build:ssr": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:server": "tsc -p tsconfig.json",
    "build": "pnpm run build:client && pnpm run build:ssr && pnpm run build:server",
    "start": "NODE_ENV=production node dist/index.js",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.40",
    "@google-cloud/firestore": "^7.9.0",
    "@google-cloud/secret-manager": "^6.1.1",
    "@hono/node-server": "^1.19.6",
    "@vitejs/plugin-react": "^4.3.4",
    "ai": "^5.0.98",
    "dotenv": "^17.2.3",
    "google-auth-library": "^10.5.0",
    "googleapis": "^166.0.0",
    "hono": "^4.10.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^5.4.10",
    "vite-ssr-components": "^0.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@types/node": "^22.8.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3"
  }
}
