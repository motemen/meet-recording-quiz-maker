{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["vite/client"],
    "noEmit": true
  },
  "include": ["src/client/**/*.ts", "src/client/**/*.tsx"],
  "exclude": []
}
